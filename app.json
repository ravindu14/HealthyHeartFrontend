{
  "expo": {
    "name": "HealthyHeartApp",
    "slug": "HealthyHeartApp",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "main"
    },
    "description": "",
    "android": {
      "package": "main"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}